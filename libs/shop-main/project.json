{
  "projectType": "library",
  "root": "libs/shop-main",
  "sourceRoot": "libs/shop-main/src",
  "prefix": "ng-module-federation",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shop-main"],
      "options": {
        "jestConfig": "libs/shop-main/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shop-main/**/*.ts",
          "libs/shop-main/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
